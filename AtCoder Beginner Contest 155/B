        //focus
        #include <bits/stdc++.h>
        #define int long long
        using namespace std;
        int x;vector<int>v;
        int32_t main(){
        ios_base::sync_with_stdio(false);cin.tie(NULL);
        cin>>x;bool ok=1;
        for(int i=0;i<x;i++){
            int f;cin>>f;
            if(f%2==0){
                if(f%3!=0&&f%5!=0){
                    ok=0;
                }
            }
        }
        if(ok){
            cout<<"APPROVED"<<endl;
        }
        else{
            cout<<"DENIED"<<endl;
        }
        return 0;
        }
